/**
 * @param {number[]} nums
 * @param {number} val
 * @return {number}
 */
var removeElement = function(nums, val) {
    let i = 0;
    while(i < nums.length){
        let num = nums[i]
        if(num == val){
            nums.splice(i, 1)
            i -= 1
        }
        i++;
    }
    return(nums.length)
};
